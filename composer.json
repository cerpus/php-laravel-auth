{
    "name": "cerpus/laravel-auth",
    "description": "Authentication plugin for Laravel",
    "authors": [
        {
            "name": "Jan-Espen Oversand",
            "email": "jan.espen.oversand@cerpus.com"
        }
    ],
    "require": {
        "php": ">=7",
        "illuminate/support": "~5.2",
        "illuminate/routing": "~5.2",
        "cerpus/cerpusauthlib-core": "0.0.18"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Cerpus\\LaravelAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cerpus\\LaravelAuth\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.cerpus.net"
        },
        {
            "type": "composer",
            "url": "https://composer-3rdparty.cerpus.net"
        }
    ]
}
